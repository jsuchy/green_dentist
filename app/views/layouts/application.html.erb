<!DOCTYPE html>
<html>
  <head>
    <title>Green Dentist</title>
    <%= stylesheet_link_tag :all %>
    <%= javascript_include_tag :defaults %>
    <%= csrf_meta_tag %>
  </head>
  <body>
    
    <div id="wrap">


      <div id="header">
        <h1><%= link_to "Green Dentist", :controller => "main", :action => "index" %></h1>
        <h2>Chairside Amalgam Separator </h2>
      </div>

      <div id="menu">
        <ul>
          <li>
            <%= link_to "Home", :controller => "main", :action => "index" %>
          </li>
          <li>
            <%= link_to "Products", :controller => "main", :action => "products" %>
          </li>
          <li>
            <%= link_to "Company", :controller => "main", :action => "company" %>
          </li>
          <li>
            <%= link_to "Links", :controller => "main", :action => "links" %>
          </li>
          <li>
            <%= link_to "Contact", :controller => "main", :action => "contact" %>
          </li>
        </ul>
      </div>

      <div id="menubottom"> </div>

      <div id="contentwrap"> 

        <%= yield %>
        
        

        <div style="clear: both;"> </div>

      </div>

      <div id="footer">
        <p>&copy; Copyright 2011 David Mendlik</p>
      </div>

    </div>

  </body>
  
</html>
